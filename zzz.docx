    "def dfs_solver_with_cost(constraints, domains, context, var_order, csp):\n",
    "    \"\"\"DFS solver with cost-based heuristics:\n",
    "       - Choose variable with minimum cost first\n",
    "       - Explore values in order of cost\n",
    "    \"\"\"\n",
    "    global num_expanded, display\n",
    "    to_eval = {c for c in constraints if c.can_evaluate(context)}\n",
    "    \n",
    "    if all(c.holds(context) for c in to_eval):\n",
    "        if var_order == []:\n",
    "            print(\"Nodes expanded to reach solution:\", num_expanded)\n",
    "            yield context\n",
    "        else:\n",
    "            rem_cons = [c for c in constraints if c not in to_eval]\n",
    "\n",
    "            def var_min_cost(var):\n",
    "                return min(value_cost(var, val, csp) for val in domains[var])\n",
    "            var = min(var_order, key=var_min_cost)\n",
    "\n",
    "            values = sorted(domains[var], key=lambda val: value_cost(var, val, csp))\n",
    "\n",
    "            new_var_order = [v for v in var_order if v != var]\n",
    "            for val in values:\n",
    "                if display:\n",
    "                    print(\"Setting\", var, \"to\", val, \"with cost\", value_cost(var, val, csp))\n",
    "                num_expanded += 1\n",
    "                yield from dfs_solver_with_cost(rem_cons, domains, context | {var: val}, new_var_order, csp)\n",
    "\n",
    "def value_cost(var, val, csp):\n",
    "    \"\"\"Return cost of assigning val to var, based on soft deadline.\"\"\"\n",
    "    if var not in csp.soft_day_time or int(csp.soft_costs[var]) == 0:\n",
    "        return 0\n",
    "    day_time = Day_Time()\n",
    "    _, soft_set = day_time.string_to_number_set(csp.soft_day_time[var])\n",
    "    deadline = next(iter(soft_set))\n",
    "    unit_cost = int(csp.soft_costs[var])\n",
    "    end = val + csp.durations[var]\n",
    "    lateness = max(0, end - deadline)\n",
    "    return lateness * unit_cost\n",
    "\n",
    "\n",
    "csp = create_CSP_from_spec(sample_spec)\n",
    "print(\"Domains:\", csp.domains)\n",
    "list(dfs_solver(csp.constraints, csp.domains, {}, list(csp.domains)))\n",
    "print(\"Nodes expanded:\", num_expanded)\n",
    "list(dfs_solver_with_cost(csp.constraints, csp.domains, {}, list(csp.domains), csp))\n",
    "print(\"new Nodes expanded:\", num_expanded)\n",
    "\n",
    "\n"
   ]
